{
  "name": "Website",
  "version": "0.0.1",
  "description": "Ow, my brand",
  "author": "Ty Cobb",
  "license": "GPL-3.0",
  "scripts": {
    "start": "webpack-dev-server --debug --hot --inline",
    "build:prod": "webpack -p",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect --debug-brk ./node_modules/.bin/jest --runInBand",
    "lint": "standard | snazzy",
    "lint:fix": "standard --fix | snazzy",
    "flow:restart": "flow stop && flow",
    "validate": "yarn lint && yarn flow && yarn test"
  },
  "dependencies": {
    "glamor": "^2.20.40",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "enzyme": "^2.9.1",
    "eslint-plugin-flowtype": "^2.35.1",
    "flow-bin": "^0.54.1",
    "jest": "^21.0.2",
    "jest-enzyme": "^3.8.2",
    "react-test-renderer": "^15.6.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1",
    "webpack-html-plugin": "^0.1.1"
  },
  "babel": {
    "presets": [
      "react",
      [ "env", { "targets": { "browsers": "> 2%" } } ],
      "stage-1"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/setupFramework.js",
    "setupFiles": [
      "./test/setup.js"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "ignore": [
      "flow-typed/"
    ]
  }
}
